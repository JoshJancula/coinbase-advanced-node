{
  "bugs": {
    "url": "https://github.com/joshjancula/coinbase-advanced-node/issues"
  },
  "dependencies": {
    "@types/node": "18.11.10",
    "@types/ws": "8.5.3",
    "axios": "1.6.7",
    "axios-retry": "3.3.1",
    "buffer": "6.0.3",
    "bufferutil": "4.0.7",
    "jsonwebtoken": "^9.0.2",
    "qs": "^6.11.2",
    "reconnecting-websocket": "4.4.0",
    "utf-8-validate": "5.0.10",
    "ws": "8.9.0"
  },
  "description": "Coinbase API for Node.js, written in TypeScript and covered by tests.",
  "devDependencies": {
    "@types/jasmine": "4.0.3",
    "@types/qs": "^6.9.11",
    "@typescript-eslint/eslint-plugin": "5.45.0",
    "@typescript-eslint/parser": "5.45.0",
    "cross-env": "7.0.3",
    "dotenv-defaults": "5.0.2",
    "eslint": "8.28.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-typescript-sort-keys": "2.1.0",
    "generate-changelog": "1.8.0",
    "husky": "8.0.2",
    "jasmine": "3.99.0",
    "lint-staged": "13.0.4",
    "nock": "13.2.9",
    "nyc": "15.1.0",
    "prettier": "2.8.0",
    "pretty-quick": "3.1.3",
    "rimraf": "3.0.2",
    "ts-node": "10.9.1",
    "typedoc": "0.23.21",
    "typedoc-plugin-markdown": "3.13.6",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">= 10.9",
    "yarn": ">= 1"
  },
  "files": [
    "dist",
    "!/dist/demo",
    "!/dist/test"
  ],
  "homepage": "https://github.com/joshjancula/coinbase-advanced-node",
  "keywords": [
    "Node",
    "Advanced",
    "Trade",
    "API",
    "Bitcoin",
    "Coinbase",
    "crypto",
    "cryptocurrency",
    "currency",
    "Ethereum",
    "finance",
    "fintech",
    "order",
    "payments",
    "trading",
    "TypeScript",
    "withdraw"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "name": "coinbase-advanced-node",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joshjancula/coinbase-advanced-node.git"
  },
  "scripts": {
    "build": "tsc",
    "changelog:commit": "git add CHANGELOG.md && git commit -m \"docs: Updated changelog\"",
    "clean": "rimraf .nyc_output coverage dist",
    "demo:dump:candles": "cross-env NODE_DEBUG=coinbase-advanced-node* ts-node src/demo/dump-candles.ts",
    "demo:rest:get:candles": "cross-env NODE_DEBUG=coinbase-advanced-node* ts-node src/demo/rest-get-candles.ts",
    "demo:rest:watch:candles": "cross-env NODE_DEBUG=coinbase-advanced-node* ts-node src/demo/rest-watch-candles.ts",
    "demo:websocket:ticker": "cross-env NODE_DEBUG=coinbase-advanced-node* ts-node src/demo/websocket-ticker.ts",
    "demo:websocket:user": "cross-env NODE_DEBUG=coinbase-advanced-node* ts-node src/demo/websocket-user.ts",
    "docs": "yarn docs:build",
    "docs:build": "typedoc src/index.ts",
    "docs:release": "yarn docs:build && git add docs/* && git commit -m \"docs: Updated API\"",
    "dist": "yarn clean && yarn build",
    "fix": "yarn fix:other && yarn fix:code",
    "fix:code": "yarn lint:code --fix",
    "fix:other": "yarn prettier --write",
    "lint": "yarn lint:types && yarn lint:code && yarn lint:other",
    "lint:code": "eslint --ignore-path .gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:other": "yarn prettier --list-different",
    "lint:types": "tsc --noEmit",
    "postversion": "git push origin && git push origin --tags && npm publish",
    "prettier": "prettier --ignore-path .gitignore --loglevel silent \"**/*.{html,json,scss,yml}\"",
    "preversion": "git checkout main && git pull && yarn install && yarn lint && yarn test && yarn dist",
    "release:major": "generate-changelog -t 1e447fd... && yarn changelog:commit && yarn docs:release && npm version major",
    "release:minor": "generate-changelog -m 1e447fd... && yarn changelog:commit && yarn docs:release && npm version minor",
    "release:patch": "generate-changelog -p && yarn changelog:commit && yarn docs:release && npm version patch",
    "test": "nyc --nycrc-path=nyc.config.coverage.json jasmine --config=jasmine.json",
    "test:dev": "nyc --nycrc-path=nyc.config.json jasmine --config=jasmine.json",
    "test:types": "yarn lint:types",
    "prepare": "husky install"
  },
  "version": "3.3.0"
}
